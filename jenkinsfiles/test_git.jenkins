            }
        }
        stage("Second step") {
            steps {
                sh 'ssh root@alexu \'uptime\''
            }
        }
        
        stage("Third step") {
            steps {
                echo "BRANCHNAME = $BRANCHNAME"
                
            }
        }
    }
}
